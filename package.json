{
  "name": "lcpsocial-network",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=1 ng serve",
    "startp": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=1 ng serve --configuration production",
    "startssl": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 ng serve --ssl true --ssl-key ssl\\server.key --ssl-cert ssl\\server.crt",
    "startsslp": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 ng serve --ssl true --ssl-key ssl\\server.key --ssl-cert ssl\\server.crt -c production",
    "startapi": "cd api\\LCPSNWebApi && dotnet watch",
    "startapip": "cd api\\LCPSNWebApi && dotnet watch --launch-profile LCPSNWebApi-Production",
    "startall": "concurrently 'npm:startapi' 'npm:start'",
    "startallp": "concurrently 'npm:startapip' 'npm:startp'",
    "startallssl": "concurrently 'npm:startapi' 'npm:startssl'",
    "startallsslp": "concurrently 'npm:startapip' 'npm:startsslp'",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --verbose",
    "testcc": "jest --coverage",
    "testccw": "jest --watch --coverage",
    "testall": "concurrently 'npm:startapi' 'npm:test'",
    "testallcc": "concurrently 'npm:startapi' 'npm:testcc'",
    "testallccw": "concurrently 'npm:startapi' 'npm:testccw'",
    "serve:ssr:LCPSocialNetwork": "node dist/lcpsocial-network/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/cdk": "^17.2.1",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/material": "^17.2.1",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/platform-server": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@angular/ssr": "^17.2.1",
    "@microsoft/signalr": "^8.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "concurrently": "^8.2.2",
    "cookieconsent": "^3.1.1",
    "express": "^4.18.2",
    "libphonenumber-js": "^1.10.58",
    "marked": "^12.0.1",
    "ngx-cookieconsent": "^6.0.0",
    "ngx-mat-intl-tel-input": "^5.0.0",
    "rxjs": "~7.8.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.1",
    "@angular/cli": "^17.2.1",
    "@angular/compiler-cli": "^17.2.0",
    "@angular/localize": "^17.2.2",
    "@types/bootstrap": "^5.2.10",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/marked": "^6.0.0",
    "@types/node": "^18.18.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.3",
    "typescript": "~5.3.2",
    "whatwg-fetch": "^3.6.20"
  }
}
